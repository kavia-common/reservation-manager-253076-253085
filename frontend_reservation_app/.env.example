# Frontend environment configuration (example)
# Copy to .env and adjust values for your environment.

# PUBLIC FACING URLS
REACT_APP_FRONTEND_URL=/
REACT_APP_API_BASE=http://localhost:4000
REACT_APP_BACKEND_URL=http://localhost:4000
REACT_APP_WS_URL=

# ENV/BUILD
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=true
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info

# HEALTHCHECK
REACT_APP_HEALTHCHECK_PATH=/health

# FEATURES
REACT_APP_FEATURE_FLAGS=reservations_polling,realtime_ws
REACT_APP_EXPERIMENTS_ENABLED=false
